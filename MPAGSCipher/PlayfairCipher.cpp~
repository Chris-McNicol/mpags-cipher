PlayfairCipher::PlayfairCipher(String thekey, CipherMode decrypt_mode)
  :key_{thekey)},mode_{decrypt_mode}
   {
   }
   

PlayfairCipher::setKey(std::string keyToSet){
  key_ = keyToSet;
}

  //applies shift for each char in a string
std::string PlayfairCipher::applyCipher(std::string msg){

  return msg;
   }    










